{
    "ver": "0.0.1",
    "authors": [
        "landml"
    ],
    "contact": "https://kbase.us/contact-us/",
    "categories": ["active"],
    "widgets": {
        "input": null,
        "output": "no-display"
    },
    "parameters": [ 
        {
            "id": "genome_input_ref",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [ "KBaseGenomes.Genome" ]
            }
        },
        {
            "id": "report_format",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [
                "tab"
            ],
            "field_type": "dropdown",
            "dropdown_options": {
                "options": [ 
					{
						"value": "tab",
						"display": "Features, tab-delimited" 
					},
					{
						"value": "csv",
						"display": "Features, comma-delimited" 
					},
					{
						"value": "gff",
						"display": "Features, GFF3 format"
					},
					{
						"value": "fasta",
						"display": "CDSs, fasta format"
					}
				]
            }
        }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name":"Report_util_landml",
            "method": "genome_report",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },
                {
                    "input_parameter": "genome_input_ref",
                    "target_property": "genome_input_ref",
                    "target_type_transform": "resolved-ref"
                },
                {
                    "input_parameter": "report_format",
                    "target_property": "report_format"
                }
            ],
            "output_mapping": [
                {
                    "service_method_output_path": [0,"report_name"],
                    "target_property": "report_name"
                },
                {
                    "service_method_output_path": [0,"report_ref"],
                    "target_property": "report_ref"
                },
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}
